{
  "kind": "singleType",
  "collectionName": "homepages",
  "info": {
    "singularName": "homepage",
    "pluralName": "homepages",
    "displayName": "Homepage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "heroText": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "heroMedia": {
      "type": "media",
      "allowedTypes": ["images", "videos"],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "experienceVideo": {
      "type": "media",
      "allowedTypes": ["videos"],
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featuredCaseStudies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::case-study.case-study"
    },
    "featuredPosts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post"
    }
  }
}
